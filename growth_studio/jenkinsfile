node{
    stage('Checkout'){
      git 'https://github.com/lethoni/growth-learn'
    }
    
    stage('Create Virtualenv'){
      bat 'bash ./growth_studio/ci/setup.sh'
    }

    stage('Unit Test'){
      bat 'bash ./growth_studio/ci/unit_test.sh'
    }
    
    stage('E2E Test'){
      bat 'bash ./growth_studio/ci/e2e.sh'
    }
    
    stage('Deploy'){
      bat 'bash ./growth_studio/ci/deploy.sh'
    }
}
